{
  "name": "@prncss-xyz/utils",
  "version": "0.0.5",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "description": "personal utilities",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prncss-xyz/utils.git"
  },
  "files": [
    "dist"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:test": "vitest watch",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "test:types": "tsc --noEmit",
    "format": "prettier --write ./{lib,src}"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.17.2",
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.7",
    "eslint": "^9.18.0",
    "eslint-plugin-perfectionist": "^4.7.0",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "knip": "^5.42.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.10",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "nanoid": "^5.0.9"
  },
  "peerDependencies": {
    "@constellar/core": "^2.4.2"
